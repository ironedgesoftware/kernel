<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="event_dispatcher" class="IronEdge\Component\Kernel\EventDispatcher\EventDispatcher" />

        <service id="cache.factory" class="IronEdge\Component\Cache\Factory" />

        <service id="logger.factory" class="IronEdge\Component\Logger\Factory" />

        <service id="logger.formatter.custom_line_formatter" class="IronEdge\Component\Kernel\Logger\Formatter\CustomLineFormatter" />
    </services>
</container>